
  // write your code here
  public static int mostFrequentElement(int[] a) {
  int count = 0, tempCount;
  int popular = a[0];
  int temp;
  
  for (int i = 0; i < (a.length ); i++)
  {
    temp = a[i];
    tempCount = 0;
    for (int j = 0; j < a.length; j++)
    {
      if (temp == a[j]){
         tempCount++;
      }
       
        //++j
    }
    if (tempCount >= count)
    {
      popular = temp;
      count = tempCount;
    }
  }//++i;
  return popular;
  
}
  
